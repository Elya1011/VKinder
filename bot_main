import os
from functions import VkBot
from time import sleep


if __name__ == '__main__':

    session = VkBot(access_token=os.getenv('VKTOKEN'))
    search_result = session.search_users(age=24, sex=1, hometown='москва')['response']['items']
    print(len(search_result))
    for profile in search_result:
        profile_pic_list = session.get_profile_pics_list(user_id=profile['id'])
        # print(profile)
        session.download_photo(user_id=profile['id'])
        sleep(0.1)